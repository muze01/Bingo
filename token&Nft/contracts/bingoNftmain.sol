    } 

    function getPrice(uint256 amount) public pure returns (uint) {
        return 55000000000000000 * amount;
    }

    function nftMint(uint _nftId, uint256 amount) public payable {
        require(started, "NOT STARTED");
        require(_nftId <= 4, "NO NFT FOR THAT ID");
        require(totalSupply(_nftId) < MAX_SUPPLY, "MAXSUPPLY REACHED FOR THIS NFT");
        // A wallet Can only own 50 max of each nft Id...total 250
        require(balanceOf(msg.sender, _nftId) < MAX_PURCHASE, "CAN'T MINT MORE");

        // 0.055eth mulfiplied by the amount of nft's
        uint256 currentPrice = getPrice(amount);
        require(msg.value >= currentPrice, "INCORRECT VALUE");
        
        _mint(msg.sender, _nftId, amount, "");
    }

    function renounceOwnership() public virtual override onlyOwner {
        _transferOwnership(msg.sender);
    }

    function pause() public onlyOwner {
       started = !started;
    }

    function withdraw() external onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    function _beforeTokenTransfer(
         address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual override (ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

}